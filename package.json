  "dependencies": {
    "gh-pages": "^6.3.0",
    "nuxt": "^3.16.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "tailwindcss": "^3.4.17",
    "vite-plugin-vuetify": "^2.1.1",
    "vuetify": "^3.8.2"
  }
}
